-- Get Robux Section:
Button = Page4:CreateButton("Tp To Richest Player", "Teleport", function()
local players = game:GetService("Players"):GetPlayers()
local marketplaceService = game:GetService("MarketplaceService")

local playerWithMostExpensiveAvatar = nil
local highestPrice = 0

for _, player in ipairs(players) do
    local userId = player.UserId
    local success, result = pcall(function()
        return marketplaceService:GetProductInfo(userId)
    end)
    
    if success then
        local price = result.PriceInRobux
        print(player.Name .. "'s avatar price: " .. tostring(price) .. " Robux")
        
        if price and price > highestPrice then
            highestPrice = price
            playerWithMostExpensiveAvatar = player
        end
    else
        warn("Failed to retrieve avatar info for player: " .. player.Name)
    end
end

if playerWithMostExpensiveAvatar then
    local character = playerWithMostExpensiveAvatar.Character
    if character then
        local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        if humanoidRootPart then
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = humanoidRootPart.CFrame
        end
    end
end
end)

Button = Page4:CreateButton("WalkTo Random Player", "Walk", function()
local player = game.Players.LocalPlayer
local players = game.Players:GetPlayers()

local randomPlayer = players[math.random(1, #players)]
local targetCharacter = randomPlayer.Character
local targetHumanoidRootPart = targetCharacter and targetCharacter:FindFirstChild("HumanoidRootPart")

if targetHumanoidRootPart then
    local playerCharacter = player.Character
    local playerHumanoidRootPart = playerCharacter and playerCharacter:FindFirstChild("HumanoidRootPart")

    if playerHumanoidRootPart then
        local distanceThreshold = 5
        local currentDistance = (targetHumanoidRootPart.Position - playerHumanoidRootPart.Position).Magnitude

        while currentDistance > distanceThreshold do
            local moveDirection = (targetHumanoidRootPart.Position - playerHumanoidRootPart.Position).Unit
            local moveSpeed = 29

            playerHumanoidRootPart.Velocity = moveDirection * moveSpeed

            currentDistance = (targetHumanoidRootPart.Position - playerHumanoidRootPart.Position).Magnitude

            wait()
        end

        playerHumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
    end
end
end)

TextBox = Page3:CreateBox("Insert Walkspeed", 10044538000, function(t)
    local player = game.Players.LocalPlayer
    local players = game.Players:GetPlayers()

    local randomPlayer = players[math.random(1, #players)]
    local targetCharacter = randomPlayer.Character
    local targetHumanoidRootPart = targetCharacter and targetCharacter:FindFirstChild("HumanoidRootPart")

    if targetHumanoidRootPart then
        local playerCharacter = player.Character
        local playerHumanoidRootPart = playerCharacter and playerCharacter:FindFirstChild("HumanoidRootPart")

        if playerHumanoidRootPart then
            local distanceThreshold = 5
            local currentDistance = (targetHumanoidRootPart.Position - playerHumanoidRootPart.Position).Magnitude

            local moveSpeedTextbox = Instance.new("TextBox")
            moveSpeedTextbox.PlaceholderText = "Move Speed"
            moveSpeedTextbox.Parent = game:GetService("CoreGui")
            moveSpeedTextbox.Position = UDim2.new(0.5, 0, 0.9, 0)
            moveSpeedTextbox.Size = UDim2.new(0, 100, 0, 20)
            moveSpeedTextbox.Text = "29"

            while currentDistance > distanceThreshold do
                local moveDirection = (targetHumanoidRootPart.Position - playerHumanoidRootPart.Position).Unit
                local moveSpeed = tonumber(moveSpeedTextbox.Text) or 0

                playerHumanoidRootPart.Velocity = moveDirection * moveSpeed

                currentDistance = (targetHumanoidRootPart.Position - playerHumanoidRootPart.Position).Magnitude

                wait()
            end

            playerHumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
            moveSpeedTextbox:Destroy()
        end
    end
end)


