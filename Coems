        "fartist",
        "pablo250",
        "monalisa200",
        "easterart",
        "picasso250",
        "pablo300",
        "bobux",
        "artcoin",
        "starving"
    }

    local remote = game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Codes")

    for i, code in ipairs(codes) do
        local args = {
            [1] = code
        }

        wait(i * 1.5)
        remote:InvokeServer(unpack(args))
    end
end)

